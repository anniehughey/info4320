// Servo - Version: Latest #include <Servo.h>/*moves the pan-tilt*/Servo panservo;Servo tiltservo;int pos = 0;void setup() {  pinMode(9, OUTPUT);  pinMode(10, OUTPUT);  panservo.attach(10);  tiltservo.attach(9);}void loop() {  for (pos = 0; pos <= 150; pos += 1) {     panservo.write(pos);  //can pan (rotate) roughly 180 degrees                    tiltservo.write(pos); //can tilt up& down roughly 150degrees          delay(10);                        }  for (pos = 150; pos >= 0; pos -= 1) {     panservo.write(pos);                          tiltservo.write(pos);     delay(10);                       }}